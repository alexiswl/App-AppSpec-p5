#!perl -w
# vim:et:sts=4:sws=4:sw=4
use strict;
use warnings;
use 5.010;
use Data::Dumper;

use App::Spec;
use App::AppSpec;
use File::Share qw/ dist_file /;

my $specfile = dist_file("App-AppSpec", "appspec-spec.yaml");
my $spec = App::Spec->read($specfile);
my $run = App::AppSpec->new({
    spec => $spec,
});
$run->run;

